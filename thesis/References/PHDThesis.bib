@mastersthesis{Traversaro2013,
abstract = {Analysis, design and control of multibody systems rely on dynamical models that de- scribe the relationship between forces acting on the bodies and the resulting motion. A dynamical model is characterized by the inertial parameters of the bodies composing the system which, in case of rigid bodies, are: masses, positions of centers of mass and rotational inertias. Inertial parameters can be computed from CAD models, but this approach often results in a poor quality of the dynamical model. Alternatively, dynamic parameters can be estimated on real systems through motion and force measurements. This thesis tackles the problem of the online estimation of inertial parameters, using the iCub humanoid robot as test platform. We review, under a unifying framework, various approaches to inertial parameter estimation, each characterized by the type of force measurements that it uses. We analyze the problem of non-identifiability of the inertial parameters, reporting the related results from the literature. Moreover, we present a method for numerically characterizing the confidence of the estimation, moving the focus from the estimated parameters to the quantities that we want to compute using them. Based on this theoretical analysis, we implemented a C++ open-source software for online estimation of inertial parameters. Tests on the arm of the iCub robot validated the presented software, showing an improvement with respect to the inertial parameters computed from the CAD model of the robot.},
author = {Traversaro, Silvio},
number = {March},
pages = {82},
school = {Universita' di Genova},
title = {{Online inertial parameter estimation with confidence bounds: theory and implementation on the iCub robot}},
year = {2013}
}
